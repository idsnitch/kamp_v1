{% extends 'base.htm.twig' %}
{% block javascript %}

    <!-- Core JS files -->
    <script type="text/javascript" src="{{ asset('loko/js/plugins/loaders/pace.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('loko/js/core/libraries/jquery.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('loko/js/core/libraries/bootstrap.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('loko/js/plugins/loaders/blockui.min.js') }}"></script>
    <!-- /core JS files -->
    <!-- Theme JS files -->
    <script type="text/javascript" src="{{ asset('loko/js/plugins/forms/wizards/steps.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('loko/js/plugins/tables/datatables/datatables.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('loko/js/plugins/forms/selects/select2.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('loko/js/plugins/forms/styling/uniform.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('loko/js/core/libraries/jasny_bootstrap.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('loko/js/plugins/forms/validation/validate.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('loko/js/plugins/extensions/cookie.js') }}"></script>

    <script type="text/javascript" src="{{ asset('loko/js/core/app.js') }}"></script>
    <script type="text/javascript" src="{{ asset('loko/js/pages/datatables_basic.js') }}"></script>
    <script type="text/javascript" src="{{ asset('loko/js/pages/wizard_steps.js') }}"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.6.0/js/bootstrap-datepicker.min.js"></script>
    <script>
        jQuery(document).ready(function() {
            $('.js-datepicker').datepicker(
                {
                    format: 'yyyy-mm-dd'
                }
            );
        });
    </script>
    <script src="{{ asset('loko/mod_vina_treeview_menus/assets/js/jquery.cookie.js') }}"
            type="text/javascript"></script>
    <script src="{{ asset('loko/mod_vina_treeview_menus/assets/js/jquery.treeview.js') }}"
            type="text/javascript"></script>
    <script type="text/javascript">
        jQuery(function () {
            jQuery(document).ready(function () {
                jQuery('.active').addClass('open');
                jQuery('.active').children('ul').slideDown();
                jQuery('#je_accord460 li.has-sub>a').on('click', function () {
                    jQuery(this).removeAttr('href');
                    var element = jQuery(this).parent('li');

                    if (element.hasClass('open')) {
                        element.removeClass('open');
                        element.find('li').removeClass('open');
                        element.find('ul').slideUp('normal');
                    }
                    else {
                        element.addClass('open');
                        element.children('ul').slideDown('normal');
                        element.siblings('li').children('ul').slideUp('normal');
                        element.siblings('li').removeClass('open');
                        element.siblings('li').find('li').removeClass('open');
                        element.siblings('li').find('ul').slideUp('normal');
                    }
                });
                jQuery('.home').addClass('active');

                jQuery('#je_accord460>ul>li.has-sub>a').append('<span class="holder"></span>');
            });
        });

        jQuery(function ($) {
            $(".hasTooltip").tooltip({"html": true, "container": "body"});
        });
    </script>
{% endblock %}
{% block title %}Home{% endblock %}
{% block pageTitle %}<h2>Home</h2>{% endblock %}
{% block breadCrumbs %}
    <ol class="breadcrumb">
        <li><i class="fa fa-home"></i></li>


        <li class="active">Home</li>
    </ol>
{% endblock %}
{% block left %}{% endblock %}
{% block component %}
<div id="sp-component" class="col-sm-12 col-md-12">
    {% endblock %}
    {% block body %}
    <div id="content" class="app-content box-shadow-z0" role="main">
        <div class="app-header white box-shadow">
            <div class="navbar">


                <!-- navbar collapse -->
                <div class="collapse navbar-toggleable-sm" id="collapse">
                    <div ui-include="'../views/blocks/navbar.form.right.html'"></div>
                    <!-- link and dropdown -->
                    <ul class="nav navbar-nav">
                        <li class="nav-item dropdown"><br/>
                            <h5>Profile Review - {{ profile.fullName }}</h5>

                        </li>
                    </ul>
                    <!-- / -->
                </div>
                <!-- / navbar collapse -->
            </div>
        </div>

        <div ui-view class="app-body" id="view">

            <!-- ############ PAGE START-->



            <div class="padding">
                <div class="panel panel-default">
                    <div  class="panel-heading"><strong>Personal Information</strong></div>
                    <div  class="panel-body">
                        <div class="row">

                            <div class="col-md-6"><strong>Names:&nbsp;&nbsp; </strong>
                                {{ profile.prefix }} {{ profile.fullName }}
                            </div>

                            <div class="col-md-6">
                                <strong>Date Of Birth</strong>
                                {% if profile.dateOfBirth %}
                                    {{ profile.dateOfBirth | date('d/m/Y')  }}
                                {%  endif %}
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6"><strong>Nationality:&nbsp;&nbsp; </strong>
                                {{ profile.nationality }}
                            </div>
                            <div class="col-md-6"><strong>Language:&nbsp;&nbsp; </strong>
                                {{ profile.language }}
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6"><strong>Marital Status:&nbsp;&nbsp; </strong>
                                {{ profile.maritalStatus }}
                            </div>
                            <div class="col-md-6"><strong>Spouse Name:&nbsp;&nbsp; </strong>
                                {{ profile.spouseName }}
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6"><strong>ID Number:&nbsp;&nbsp; </strong>
                                {{ profile.idNumber }}
                            </div>
                            <div class="col-md-6"><strong>KRA Itax PIN:&nbsp;&nbsp; </strong>
                                {{ profile.itaxPin }}
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-6"><strong>Gender:&nbsp;&nbsp; </strong>

                                {{ profile.gender }}

                            </div>
                            <div class="col-md-6">
                                <strong>Rights:&nbsp;&nbsp; </strong>
                                {% for profileRight in profile.rights %}
                                    <br/>{{ profileRight }}
                                {% endfor %}

                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6"><strong>Country of Residence:&nbsp;&nbsp; </strong>
                                {{ profile.countryOfResidence }}
                            </div>
                            <div class="col-md-6"><strong>Source of Data:&nbsp;&nbsp; </strong>
                                {% if profile.sourceOfData %}
                                    {{ profile.sourceOfData }}
                                {% else %}
                                    Self
                                {% endif %}
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6"><strong>Registration Date:&nbsp;&nbsp; </strong>

                                {{ profile.createdAt | date('d/m/Y')  }}

                            </div>
                            <div class="col-md-6"><strong>Status:&nbsp;&nbsp; </strong>
                                {% if profile.isPaid %}
                                    Paid Membership
                                {% else %}
                                    Payment Pending
                                {% endif %}

                            </div>
                        </div>

                    </div>
                </div>
                <div class="panel panel-default">
                    <div class="panel-heading"><strong>Address Details</strong></div>
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-md-6"><strong>Physical Address:&nbsp;&nbsp; </strong>
                                {{ profile.physicalAddress }}
                            </div>
                            <div class="col-md-6"><strong>Postal Address:&nbsp;&nbsp; </strong>
                                {{ profile.postalAddress }}
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-6">
                                <strong>Postal Code:&nbsp;&nbsp; </strong>
                                {{ profile.postalCode }}

                            </div>

                            <div class="col-md-6">
                                <strong>City:&nbsp;&nbsp; </strong>
                                {{ profile.city }}

                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-6">
                                <strong>Country:&nbsp;&nbsp; </strong>
                                {{ profile.country }}

                            </div>
                            <div class="col-md-6">
                                <strong>Phone Number:&nbsp;&nbsp; </strong>
                                {{ profile.phoneNumber }}

                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <strong>Alt. Phone Number:&nbsp;&nbsp; </strong>
                                {{ profile.altPhoneNumber }}

                            </div>
                            <div class="col-md-6">
                                <strong>Fax Number:&nbsp;&nbsp; </strong>
                                {{ profile.faxNumber }}

                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <strong>Email Address:&nbsp;&nbsp; </strong>
                                {{ profile.emailAddress }}

                            </div>
                            <div class="col-md-6">
                                <strong>Website:&nbsp;&nbsp; </strong>
                                {{ profile.website }}

                            </div>
                        </div>
                    </div>
                </div>
                <div class="panel panel-default">
                    <div class="panel-heading"><strong>Bank Details</strong></div>
                    <div class="panel-body">

                        <div class="row">
                            <div class="col-md-6"><strong>Name of Payee:&nbsp;&nbsp; </strong>
                                {{ profile.nameOfPayee }}
                            </div>
                            <div class="col-md-6"><strong>Account Name:&nbsp;&nbsp; </strong>
                                {{ profile.accountName }}
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-6">
                                <strong>Bank:&nbsp;&nbsp; </strong>
                                {{ profile.bank }}

                            </div>

                            <div class="col-md-6">
                                <strong>Bank Branch:&nbsp;&nbsp; </strong>
                                {{ profile.bankBranch }}

                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <strong>Account Number:&nbsp;&nbsp; </strong>
                                {{ profile.accountNumber }}

                            </div>

                            <div class="col-md-6">
                                <strong>Currency:&nbsp;&nbsp; </strong>
                                {{ profile.currency }}

                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <strong>Account Type:&nbsp;&nbsp; </strong>
                                {{ profile.bankAccountType }}

                            </div>

                            <div class="col-md-6">
                                <strong>Bank P.O Box:&nbsp;&nbsp; </strong>
                                {{ profile.bankPostalAddress }}

                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <strong>IBAN:&nbsp;&nbsp; </strong>
                                {{ profile.iban }}

                            </div>

                            <div class="col-md-6">
                                <strong>Swift BIC:&nbsp;&nbsp; </strong>
                                {{ profile.swiftBic }}

                            </div>
                        </div>
                    </div>
                </div>


                <div class="panel panel-default">
                    <div class="panel-heading"><strong>Approval</strong></div>
                    <div class="panel-body">

                        <div class="row">
                            <div class="col-md-6">
                                {% if profile.profileStatus == "Rejected" %}
                                    <strong>Rejected On:</strong> {{ profile.processedAt | date("F jS \\a\\t g:ia") }}<br/>
                                    <strong>Rejected By: </strong> {{ profile.processedBy.fullName }}<br/>
                                    <strong>Reason:</strong> {{ profile.statusDescription }}
                                {% elseif profile.profileStatus == "Approved" %}
                                    <strong>Approved On:</strong> {{ profile.processedAt | date("F jS \\a\\t g:ia") }}<br/>
                                    <strong>Approved By: </strong> {{ profile.processedBy.fullName }}<br/>
                                    <strong>Comment:</strong> {{ profile.statusDescription }}


                                {% else %}
                                    <strong>Status: </strong> {{ profile.profileStatus }}
                                {% endif %}
                            </div></div>

                    </div>
                </div>



            </div>
        </div>

    </div>
</div>

<!-- ############ PAGE END-->

    </div>
    </div>
    {% endblock %}
    {% block right %}
</div>
{% endblock %}
